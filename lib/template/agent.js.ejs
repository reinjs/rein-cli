module.exports = async app => {
  app.prepare(async () => app.logger.info(`[${app.pid}]`, 'in prepare lifecycle.'));
  app.started(async () => app.logger.info(`[${app.pid}]`, 'in started lifecycle.'));
  app.ready(async () => app.logger.info(`[${app.pid}]`, 'in ready lifecycle.'));
  app.destroyed(async () => app.logger.info(`[${app.pid}]`, 'in destroyed lifecycle.'));
};