const Cluster = require('@reinjs/rein-cluster');
const cluster = new Cluster({
  cwd: __dirname,
  <% if (agents) { %>agents: <%- JSON.stringify(agents)%>,<% } %>
  <% if (maxWorkers) { %>maxWorkers: <%= maxWorkers%>,<% } %>
  timeout: <%= timeout%>,
  framework: '<%- framework%>'
});

cluster.listen()
  .then(() => console.log('cluster listened success.'))
  .catch(e => console.error(e));