const Cluster = require('@reinjs/rein-cluster');
const reins = require('./.reinrc.json');
const cluster = new Cluster({
  cwd: __dirname,
  agents: reins.configs.agents,
  timeout: reins.configs.timeout,
  maxWorkers: reins.configs.maxWorkers,
  framework: '<%- framework%>',
  socket: reins.socket
});

cluster.listen()
  .then(() => console.log('cluster listened success.'))
  .catch(e => console.error(e));