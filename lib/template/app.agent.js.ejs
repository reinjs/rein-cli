const Agent = require('@reinjs/rein-class/agent');

module.exports = class Message extends Agent {
  constructor(...args) {
    super(...args);
  }

  /**
   * 测试事件
   * @param data 事件数据源
   * @returns {Promise[void]}
   */
  async ['test:event'](data) {
    return await app.until(async dbo => {
      return 'hello world - test event:' + JSON.stringify(data);
    });
  }
};