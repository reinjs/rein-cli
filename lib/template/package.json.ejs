{
  "name": "<%- name %>",
  "version": "1.0.0",
  "description": "project of reinjs",
  "author": "reinjs",
  "license": "MIT",
  "dependencies": <%- JSON.stringify(modules, null, 2) %>,
  <%if (isPlugin) {%>
  "devDependencies": {
    "@reinjs/rein-cli": "^1.0.5"
  },
  "plugin": {
    "name": "<%= type%>"
  },
  <%}%>
  "scripts": {
    <%if (isPlugin) {%>"postinstall": "./node_modules/.bin/rc init <%- name %>",<%}%>
    "dev": "NODE_ENV=dev node index.js --service.name=<%- name %> --service.tag=dev --service.port=8080"
  }
}
